chars=input()
def find_missing_letter(chars):
    missingChar = ''
    for i in range(0,len(chars)-1):
        if(ord(chars[i+1]) - ord(chars[i]) > 1):
            missingChar = chr(ord(chars[i])+1)
            print(missingChar)



        elif (not (ord(chars[i + 1]) - ord(chars[i]) > 1)):
            print("none")



find_missing_letter(chars)
